{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "off",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "import/prefer-default-export": "warn",
    "consistent-return": "warn",
    "no-nested-ternary": "warn"
  }
}
